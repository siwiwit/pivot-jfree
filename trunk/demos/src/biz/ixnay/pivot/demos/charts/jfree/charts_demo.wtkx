<Window title="Charts Demo" maximized="true"
    xmlns:wtkx="http://incubator.apache.org/pivot/wtkx/1.1"
    xmlns="pivot.wtk" xmlns:charts="pivot.charts">
    <content>
        <Border styles="{padding:6}">
            <content>
                <TabPane>
                    <tabs>
                        <wtkx:include src="pie_charts.wtkx" wtkx:id="pieCharts" TabPane.name="Pie Chart"/>
                        <wtkx:include src="bar_charts.wtkx" wtkx:id="barCharts" TabPane.name="Bar Chart"/>
                        <wtkx:include src="line_charts.wtkx" wtkx:id="lineCharts" TabPane.name="Line Chart"/>
                        <wtkx:include src="area_charts.wtkx" wtkx:id="areaCharts" TabPane.name="Area Chart"/>
                        <wtkx:include src="high_low_charts.wtkx" wtkx:id="highLowCharts" TabPane.name="High Low Chart"/>
                    </tabs>
                </TabPane>
            </content>
        </Border>
    </content>
</Window>



